{
    "name": "lara-zeus/wind",
    "description": "laravel packages to create and manage contact us for your site",
    "keywords": [
        "laravel",
        "lara-zeus",
        "contact",
        "form",
        "forms",
        "mailing"
    ],
    "homepage": "https://github.com/lara-zeus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/imaddev/lara-zeus-core"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^2.10",
        "lara-zeus/core": "^1.0.3"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Wind\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Wind\\WindServiceProvider"
            ],
            "aliases": {
                "Wind": "LaraZeus\\Wind\\WindFacade"
            }
        }
    }
}
